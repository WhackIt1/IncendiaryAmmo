<khaine>
	<!-- Adding Steel Ammo to ui_display so stats display correctly -->
	<append xpath="/ui_display_info/item_display">
		<item_display_info display_type="ammoBulletIncendiary" display_group="groupAmmo">
			<display_entry name="EntityDamage" title_key="statEntityDamageRanged"/>
			<display_entry name="BlockDamage" title_key="statBlockDamage"/>
			<display_entry name="dEntityDamageFire" title_key="statEntityDamageFire"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
	</append>
</khaine>